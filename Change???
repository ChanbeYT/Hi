local webhookUrl = "https://discord.com/api/webhooks/1224650430575935541/U3UESjDYWbamJjUFAm7uwnHD3tD4iOAxJr0i2M-frrkWXkrazQubAxWfBFdACn_lNZtZ"

function sendToDiscord(value)
    local httpService = game:GetService("HttpService")
    local data = {
        content = value
    }
    local json = httpService:JSONEncode(data)
  
    local response = httpService:PostAsync(webhookUrl, json)
    if response == "200" then
        print("Value sent to Discord: " .. value)
    else
        print("Error sending value to Discord")
    end
end

