Tab3:AddButton({
	Name = "Copy script that makes you teleport whre you are standing",
	Callback = function()
      		
-- Function to copy the teleport script to the clipboard
local function copyTeleportScriptToClipboard()
    local player = game.Players.LocalPlayer
    local character = player.Character
    if character then
        local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
        if humanoidRootPart then
            local position = humanoidRootPart.Position
            local scriptContent = string.format([[
                local player = game.Players.LocalPlayer
                local character = player.Character
                if character then
                    local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
                    if humanoidRootPart then
                        humanoidRootPart.CFrame = CFrame.new(Vector3.new(%f, %f, %f))
                    end
                end
            ]], position.X, position.Y, position.Z)
            setclipboard(scriptContent)
        end
    end
end

copyTeleportScriptToClipboard()
  	end    
})
